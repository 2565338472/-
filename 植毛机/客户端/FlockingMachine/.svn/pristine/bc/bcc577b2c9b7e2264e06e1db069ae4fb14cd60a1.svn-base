html,body{/* background-color:#1A4371; */ background-color:#fdfbae;padding:0;margin:0;width:100%;height:100%;font-family:microsoft yahei;font-size:15px;}
.header{height:152px;overflow:hidden;float:left;width:100%;}
.header_zhishiban{background:url(../image/ethernet.png);background-repeat:no-repeat;width:100px;float:left;color:#EEE;font-size:1.2em;margin-left:10px;margin-top:68px;padding-left:80px;	padding-top:20px;padding-bottom:10px;}
.header_logo{height:100%;width:800px;margin:0 auto;}
.header_haoxinqin_logo{height:100%;float:left;}
.header_haoxinqin_logo >img{ margin-top:50px;}
.header_title{font-size:2.3em;color:red;font-weight:bolder;}
.header_contact{color:red;font-size:1em;margin-top:6px;}
.header_sansan_logo >img{width:94px;margin-top:24px;}
.header_nav{float:right;text-align:right;}
.header_menu{margin:20px 10px 10px 0;}
.header_menu >button{border:none;outline:none;border-radius:8px;opacity:0.9;box-shadow:3px 3px 6px #333;background:rgba(250,250,255,.4);cursor:pointer;margin-right:2px;}
.header_menu >button >img{width:40px;}
.header_menu >button:hover{opacity:1;box-shadow:3px 3px 6px #000;background:rgba(255,255,255,.2);}
.header_status ul, .header_status li{padding:0;margin:0;list-style:none;}
.header_status li{float:left;overflow:hidden;width:94px;}
.header_status li>span{display:block;color:#EEE; text-align:center; line-height:24px;}
.header_status li>span.block_blur{width:40px;height:40px;border:2px solid #FFF;border-radius:8px;float:left;background:#007AFF;}
.header_status li>span.block_green{width:40px;height:40px;border:2px solid #FFF;border-radius:8px;float:left;background:#63DA38;}
.header_status li>span.block_orange{width:40px;height:40px;border:2px solid #FFF;border-radius:8px;float:left;background:#FF9500;}
.header_status li>span.block_red{width:40px;height:40px;border:2px solid #FFF;border-radius:8px;float:left;background:#F31919;}
.header_status li>span.block_gray{width:40px;height:40px;border:2px solid #FFF;border-radius:8px;float:left;background:#AAAEB4;}
.main{width:100%;height:100%;padding:152px 10px 0;box-sizing:border-box;}
.main >table{width:100%;height:100%;overflow:hidden;box-sizing:border-box;background:#FFF;}
.main >table >thead{background:#2A333D;height:30px;line-height:30px;color:#CACAD0;}
.main >table td{border-left:1px solid #808080;border-bottom:1px solid #808080;font-size:1.8em;text-align:center;}
.div_shade{background:#000;position:absolute;top:0;left:0;width:100%;height:100%;opacity:.6;display:none;}
.window{position:absolute;width:500px;height:300px;background:#FFF;top:50%;left:50%;border-radius:8px;margin-top:-150px;margin-left:-250px;box-shadow:5px 5px 10px #333;display:none;}
.window_title{background:#1A4371;font-size:1.2em;height:50px;border-top-left-radius:7px;border-top-right-radius:7px;line-height:50px;color:#DDD;padding:0 20px;}
.close_window{font-size:1.4em;color:#FFF;cursor:pointer;position:absolute;right:16px;display:inline-block;background:#F54E4E;height:30px;width:30px;text-align:center;line-height:26px;vertical-align:middle;margin-top:10px;border-radius:6px;}
.window_body{padding:20px;box-sizing:border-box;}
.window_footer{text-align:right;padding:20px;}
.window_footer button{font-size:1.4em;}
.devlog_window{width:86%;margin-left:-43%;height:700px;margin-top:-350px;}
.devlog_window table{width:100%;border-top:1px solid #808080;border-right:1px solid #808080;}
.devlog_window td,.devlog_window th{border-left:1px solid #808080;border-bottom:1px solid #808080;}
.devlog_window td{padding-left:0.5em;}
.login_window label{width:100px;display:inline-block;text-align:center;}
.login_window label,.login_window input{line-height:40px;font-size:1.4em;margin-top:10px;}
.modifyPwd_window{width:600px;margin-left:-300px;height:360px;margin-top:-180px;}
.modifyPwd_window label{width:160px;display:inline-block;}
.modifyPwd_window label,.modifyPwd_window input{line-height:40px;font-size:1.4em;margin-top:10px;}
.setting_window{width:86%;margin-left:-43%;height:700px;margin-top:-350px;}
.setting_window table{line-height:28px;width:100%;border-top:1px solid #808080;border-right:1px solid #808080;}
.setting_window table thead{line-height:40px;background:#EEE;}
.setting_window td,.setting_window th{border-left:1px solid #808080;border-bottom:1px solid #808080;text-align:center;}
.setting_window td input{border:none;outline:none;line-height:40px;text-align:center;font-size:0.9em;}
.setting_window td input[type='number']{max-width:120px;}
.setting_window td input.line_ip{width:150px;}
.setting_window h2{margin-top:0;margin-bottom:10px;}
.hide{display:none;}
td.offline{color:#AAAEB4;}
td.working{color:#63DA38;}
td.error{color:#F31919;}
td.free{color:#FF9500;}
#realdata_table td.warkingTime{font-size:1.5em;max-width:160px;color:#007AFF;}
#realdata_table >tbody >tr >td:first-child{color:#007AFF;}
#devlog_win{padding:12px;font-size:0.9em;color: #FFF;font-weight: bold;vertical-align: middle;margin-top: -24px;}

#container{width:100%;height:100%;position:fixed;top:0;left:0;z-index:999;font-size:13px;font-family:microsoft yahei;}
#output{width:100%;height:100%}
.prompt{width:60%;height:30px;margin:5px auto;text-align:center;line-height:30px;font-family:microsoft yahei,Arial,sans-serif;font-size:13px;color:#df0000}
.containerT{width:400px;height:300px;text-align:center;position:absolute;top:50%;left:50%;margin:-150px 0 0 -200px;border-radius:3px}
.containerT h1{font-size:32px;font-family:microsoft yahei,Arial,sans-serif;-webkit-transition-duration:1s;transition-duration:1s;-webkit-transition-timing-function:ease-in-put;transition-timing-function:ease-in-put;font-weight:500}
form{padding:20px 0;position:relative;z-index:2}
form input{-webkit-appearance:none;-moz-appearance:none;appearance:none;outline:0;border:1px solid rgba(255,255,255,.4);background-color:rgba(255,255,255,.2);width:200px;border-radius:3px;padding:8px 15px;margin:0 auto 10px;display:block;text-align:center;font-size:15px;color:#fff;-webkit-transition-duration:.25s;transition-duration:.25s;font-weight:300}
form input:hover{background-color:rgba(255,255,255,.4)}
form input:focus{background-color:#fff;width:230px;color:#333}
form button{-webkit-appearance:none;-moz-appearance:none;appearance:none;outline:0;background-color:#fff;border:0;padding:10px 15px;color:#333;border-radius:3px;width:200px;cursor:pointer;font-family:microsoft yahei,Arial,sans-serif;font-size:16px;font-weight:700;-webkit-transition-duration:.25s;transition-duration:.25s}
form button:hover{background-color:#f5f7f9}
.note_dialog{position:absolute;width:10em;height:3em;text-align:center;background:rgba(0,0,0,.6);color:#FFF;font-size:3em;top:50%;left:50%;margin-top:-1.5em;margin-left:-5em;line-height:3em;border-radius:8px;display:none;}